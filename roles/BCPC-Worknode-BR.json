{
  "name": "BCPC-Worknode-BR",
  "description": "A functional compute node in a BCPC cluster (RH-variant)",
  "json_class": "Chef::Role",
  "default_attributes": {
    "bcpc": {
      "management": {
        "interface": "enp0s3"
      },
      "storage": {
        "interface": "enp0s8"
      },
      "floating": {
        "interface": "enp0s9"
      }
    }
  },
  "override_attributes": {
    "bcpc": {
      "management": {
        "interface": "enp0s3"
      }
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[chef-client::delete_validation]",
    "recipe[chef-client::config]",
    "recipe[bcpc::default]",
    "recipe[bcpc::ceph-common]",
    "recipe[bcpc::ceph-work]"
  ],
  "env_run_lists": {
  }
}
